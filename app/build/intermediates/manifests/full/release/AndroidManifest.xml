<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fengodchen.infinitecalculator"
    android:versionCode="12"
    android:versionName="3.2" >

    <uses-sdk
        android:minSdkVersion="22"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_web"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_web_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.fengodchen.infinitecalculator.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fengodchen.infinitecalculator.AboutActivity"
            android:label="@string/label_about" />
        <activity
            android:name="com.fengodchen.infinitecalculator.SettingActivity"
            android:label="@string/label_setting" />
        <activity android:name="com.fengodchen.infinitecalculator.ResultActivity" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />
        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />
    </application>

</manifest>